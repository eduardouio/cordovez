<form action="recibe.php">
	<div class="row">
		<div class="col-lg-2">
			<div class="form-group">
				<label>Nro Pedido</label>
				<input 
				class="form-control" 
				type="text"
				required="true"
				name="pedido.nro_pedido"
				ng-model ="pedido.nro_pedido"
				>
			</div>
		</div>
		<div class="col-lg-2">
			<div class="form-group">
				<label>Régimen</label>
				<select
				class="form-control" 
				required="true" 
				name="pedido.regimen"
				ng-model="pedido.regimen"
				>
				<option value="" disabled selected>Seleccione ...</option>
				<option value="70">Régimen 70</option>
				<option value="10">Régimen 10</option>
				</select>
			</div>
		</div>
	<div class="col-lg-2">
      <div class="form-group">
         <label >Incoterm</label>
         <select
            class = "form-control"
            name = "incoterm.incoterms"
            required="true" 
            ng-model= "incoterm.incoterms"
            >
            <option value="" disabled selected>Seleccione ...</option>
            <option value="CFR">CFR</option>
            <option value="FCA">FCA</option>
            <option value="FOB">FOB</option>
            <option value="EXW">EXW</option>
         </select>
      </div>
   </div>
   		<div class="col-lg-2">
			<div class="form-group">
				<label>Gastos Origen (USD)</label>
				<input 
				class="form-control" 
				type="text"
				required="true"
				name="pedido.tarifa_antes_fob"
				ng-model ="pedido.tarifa_antes_fob"
				>
			</div>
		</div>
				<div class="col-lg-2">
			<div class="form-group">
				<label>Flete Intern (USD)</label>
				<input 
				class="form-control" 
				type="text"
				required="true"
				name="pedido.nro_pedido"
				ng-model ="pedido.nro_pedido"
				>
			</div>
		</div>
</div>

<input 
         type="hidden" 
         name="pedido.id_user"
         ng-model="pedido.id_user"
         value="{[pedido.id_user]}">
   <input 
         type="hidden" 
         name="pedido.last_update"
         ng-model="pedido.last_update"
         value="0000-00-00 00:00:00">
   <div class="row">
      <div class="col-lg-12">
         <button type="submit" class="btn btn-default">
         <span class="fa fa-save fa-fw"></span>
         Guardar Registro</button>
         <button type="reset" class="btn btn-default">
         <span class="fa fa-refresh fa-fw"></span>
         Limpiar Formulario</button>
         <a class="btn btn-info pull-right">
         <span class="fa fa-warning fa-fw"></span>
         Cancelar </a>
      </div>
   </div>
</form>
